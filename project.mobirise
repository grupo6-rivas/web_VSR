          "_sourceTheme": "mobirise5",
          "_customHTML": "<section data-bs-version=\"5.1\" class=\"content5\" group=\"Content\">\n    <mbr-parameters>\n        <header>Size</header>\n        <input type=\"checkbox\" title=\"Full Width\" name=\"fullWidth\">\n        <input type=\"range\" inline title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"10\" step=\"1\" value=\"0\">\n        <input type=\"range\" inline title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"10\" step=\"1\" value=\"1\">\n        <input type=\"range\" name=\"textWidth\" title=\"Width\" min=\"4\" max=\"12\" value=\"11\" step=\"1\">\n        <header>Show/Hide</header>\n        <input type=\"checkbox\" title=\"Title\" name=\"showTitle\">\n        <input type=\"checkbox\" title=\"Subtitle\" name=\"showSubtitle\">\n        <header>Background</header>\n        <input type=\"color\" title=\"Color\" name=\"bgColor\" value=\"#ffffff\">\n    </mbr-parameters>\n    <div mbr-class=\"{'container': !fullWidth, 'container-fluid': fullWidth}\">\n        <div class=\"row justify-content-center\">\n            <div class=\"col-md-12 col-lg-{{textWidth}}\">\n                <h3 class=\"mbr-section-title mbr-fonts-style mb-4\" mbr-if=\"showTitle\" data-app-selector=\".mbr-section-title\" mbr-theme-style=\"display-2\">\n                    <b>Article Title</b>\n                </h3>\n                <h4 class=\"mbr-section-subtitle mbr-fonts-style mb-4\" mbr-if=\"showSubtitle\" data-app-selector=\".mbr-section-subtitle\" mbr-theme-style=\"display-2\"><b>\n                    SIGNOS VITALES</b></h4>\n                <p class=\"mbr-text mbr-fonts-style\" data-app-selector=\".mbr-text\" mbr-theme-style=\"display-7\"><br><b>Código implementado</b><br><br>#include <wire.h>\n<br>#include \"MAX30105.h\"\n<br>#include \"spo2_algorithm.h\"\n<br>MAX30105 particleSensor;\n<br>//RECORDAR: En este caso, el sensor se comunicara con el arduino atravez de la comunicacion I2C, por lo que las conexiones de transferencia de data del\n<br>//del sensor al arduino se haran conectando los puertos A5, A4, y GND.\n<br>//El sensor trabaja con 5v\n<br>#define MAX_BRIGHTNESS 255\n<br>#if defined(__AVR_ATmega328P__) || defined(__AVR_ATmega168__)\n<br>//Arduino Uno doesn't have enough SRAM to store 100 samples of IR led data and red led data in 32-bit format\n<br>//To solve this problem, 16-bit MSB of the sampled data will be truncated. Samples become 16-bit data.\n<br>//unit16_t es un tipo de variable de 16 bits para almacenar numeros, en este caso defino como array de 100 espacios.\n<br>uint16_t irBuffer[100]; //infrared LED sensor data\n<br>uint16_t redBuffer[100];  //red LED sensor data\n<br>#else\n<br>uint32_t irBuffer[100]; //infrared LED sensor data\n<br>uint32_t redBuffer[100];  //red LED sensor data\n<br>#endif\n<br>//Defino varialbles de esas dimensiones. Optimizo espacio\n<br>int32_t bufferLength; //data length\n<br>int32_t spo2; //SPO2 value\n<br>int8_t validSPO2; //indicator to show if the SPO2 calculation is valid\n<br>int32_t heartRate; //heart rate value\n<br>int8_t validHeartRate; //indicator to show if the heart rate calculation is valid\n<br>\n<br>//Guardo valores con constantes\n<br>byte pulseLED = 11; //Must be on PWM pin\n<br>byte readLED = 13; //Blinks with each data read\n<br>void setup()\n<br>{\n<br>  Serial.begin(115200); // initialize serial communication at 115200 bits per second:\n<br>  pinMode(pulseLED, OUTPUT);\n<br>  pinMode(readLED, OUTPUT);\n<br>  // Initialize sensor\n<br>  if (!particleSensor.begin(Wire, I2C_SPEED_FAST)) //Use default I2C port, 400kHz speed\n<br>  {\n<br>    Serial.println(F(\"MAX30105 no fue conectado. Revisar conexiones de hardware.\"));\n<br>    while (1);\n<br>  }\n<br>  Serial.println(F(\"Sensor conectado. Coloque su dedo y mantengalo estable. Presione tecla para empezar\"));\n<br>  while (Serial.available() == 0) ; //Espero la comunicacion serial (Se ingrese alguna valor)\n<br>  Serial.read();//Leo la info ingresada\n<br>  byte ledBrightness = 60; //Options: 0=Off to 255=50mA\n<br>  byte sampleAverage = 4; //Options: 1, 2, 4, 8, 16, 32\n<br>  byte ledMode = 2; //Options: 1 = Red only, 2 = Red + IR, 3 = Red + IR + Green\n<br>  byte sampleRate = 100; //Options: 50, 100, 200, 400, 800, 1000, 1600, 3200. Frecuencia de muestreo del sensor\n<br>  int pulseWidth = 411; //Options: 69, 118, 215, 411\n<br>  int adcRange = 4096; //Options: 2048, 4096, 8192, 16384\n<br>  particleSensor.setup(ledBrightness, sampleAverage, ledMode, sampleRate, pulseWidth, adcRange); //Configure sensor with these settings\n<br>}\n<br>void loop()\n<br>{\n<br>  bufferLength = 100; //buffer length of 100 stores 4 seconds of samples running at 25sps\n<br>  //read the first 100 samples, and determine the signal range\n<br>  for (byte i = 0 ; i &lt; bufferLength ; i++)\n<br>  {\n<br>    while (particleSensor.available() == false) //Se ingreso data al sensor?\n<br>      particleSensor.check(); //Reviso el sensor por si esta vez si se ingreso data\n<br>    redBuffer[i] = particleSensor.getRed(); //Almaceno la data en los arrays correspondientes\n<br>    irBuffer[i] = particleSensor.getIR();\n<br>    particleSensor.nextSample(); //We're finished with this sample so move to next sample\n<br>    //Imprimo los valores sensados\n<br>    Serial.print(F(\"red=\"));\n<br>    Serial.print(redBuffer[i], DEC);\n<br>    Serial.print(F(\", ir=\"));\n<br>    Serial.println(irBuffer[i], DEC);\n<br>  }\n<br>  //calculate heart rate and SpO2 after first 100 samples (first 4 seconds of samples), Despues de 4 segundos de muestrear, calculo la saturacion y pulsaciones\n<br>  maxim_heart_rate_and_oxygen_saturation(irBuffer, bufferLength, redBuffer, &amp;spo2, &amp;validSPO2, &amp;heartRate, &amp;validHeartRate);\n<br>  //Continuously taking samples from MAX30102.  Heart rate and SpO2 are calculated every 1 second\n<br>  while (1)\n<br>  {\n<br>    //dumping the first 25 sets of samples in the memory and shift the last 75 sets of samples to the top\n<br>    //Esta parte utilizo los ultimos 75 valores + 25 valores nuevos para calcular los nuevos valores de saturacion de oxigeno y \n<br>    //pulsaciones por minuto.\n<br>    for (byte i = 25; i &lt; 100; i++)\n<br>    {\n<br>      redBuffer[i - 25] = redBuffer[i];\n<br>      irBuffer[i - 25] = irBuffer[i];\n<br>    }\n<br>    //take 25 sets of samples before calculating the heart rate.\n<br>    for (byte i = 75; i &lt; 100; i++)\n<br>    {\n<br>      while (particleSensor.available() == false) //do we have new data?\n<br>        particleSensor.check(); //Check the sensor for new data\n<br>      digitalWrite(readLED, !digitalRead(readLED)); //Blink onboard LED with every data read\n<br>      redBuffer[i] = particleSensor.getRed();\n<br>      irBuffer[i] = particleSensor.getIR();\n<br>      particleSensor.nextSample(); //We're finished with this sample so move to next sample\n<br>      //send samples and calculation result to terminal program through UART\n<br>      Serial.print(F(\"red=\"));\n<br>      Serial.print(redBuffer[i], DEC);\n<br>      Serial.print(F(\", ir=\"));\n<br>      Serial.print(irBuffer[i], DEC);\n<br>      Serial.print(F(\", HR=\"));\n<br>      Serial.print(heartRate, DEC);\n<br>      Serial.print(F(\", HRvalid=\"));\n<br>      Serial.print(validHeartRate, DEC);\n<br>      Serial.print(F(\", SPO2=\"));\n<br>      Serial.print(spo2, DEC);\n<br>      Serial.print(F(\", SPO2Valid=\"));\n<br>      Serial.println(validSPO2, DEC);\n<br>    }\n<br>    //Despues de almacenar los nuevos 25 valores, calculo el SPO2 y la frecuencia cardiaca.\n<br>    maxim_heart_rate_and_oxygen_saturation(irBuffer, bufferLength, redBuffer, &amp;spo2, &amp;validSPO2, &amp;heartRate, &amp;validHeartRate);\n<br>  }\n<br>}\n<br></wire.h><br></p>\n            </div>\n        </div>\n    </div>\n</section>",
